<Window x:Class="Swd.Bsp.Binding.fObjectBinding"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Swd.Bsp.Binding"
        mc:Ignorable="d"
        Title="fObjectBinding" Height="450" Width="800">
    <StackPanel Margin="10">
        <TextBlock Text="{Binding Id}" Margin="5"/>
        <TextBlock Text="{Binding Name}" Margin="5"/>
        <TextBlock Text="{Binding Email}" Margin="5"/>
        <TextBlock Text="{Binding PhoneNumber}" Margin="5"/>
        <Button x:Name="btnGetCustomer" Content="KundeAnzeigen" Click="btnGetCustomer_Click"/>
        <Button x:Name="btnBack" Grid.Row ="4" Content="Back ..." Margin="0" Click="btnBack_Click" />
    </StackPanel>
</Window>
